        "cross-env": "^7.0.3",
        "rimraf": "^6.0.1",
        "rollup": "^4.24.0",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-esbuild": "^6.1.1",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3",
        "vitest": "^3.0.5"
    },
    "description": "prettier plugin for .qk file formatting"
