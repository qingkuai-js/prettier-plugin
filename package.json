{
    "name": "prettier-plugin-qingkuai",
    "version": "1.0.13",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.js",
    "scripts": {
        "dev": "rollup -c -w",
        "test": "npm run build-base && npx vitest --run",
        "build": "cross-env BUILD_TYPES=1 npm run build-base",
        "build-base": "rimraf dist && npx tsc && rollup -c && rimraf dist/types"
    },
    "keywords": [
        "formatting",
        "qingkuai",
        "prettier plugin"
    ],
    "dependencies": {
        "@babel/code-frame": "^7.26.2",
        "lines-and-columns": "^2.0.4"
    },
    "devDependencies": {
        "@types/babel__code-frame": "^7.0.6",
        "@types/node": "^22.13.1",
        "cross-env": "^7.0.3",
        "prettier": "^3.5.3",
        "qingkuai": "^1.0.14",
        "rimraf": "^6.0.1",
        "rollup": "^4.24.0",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-esbuild": "^6.1.1",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3",
        "vitest": "^3.0.5"
    },
    "peerDependencies": {
        "prettier": "^3.5.3",
        "qingkuai": "^1.0.14"
    },
    "description": "prettier plugin for qingkuai(.qk) file formatting"
}
