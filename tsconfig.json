{
    "compilerOptions": {
        "strict": true,
        "target": "ESNext",
        "module": "ESNext",
        "outDir": "dist/types",
        "declaration": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "emitDeclarationOnly": true,
        "moduleResolution": "Bundler",
        "forceConsistentCasingInFileNames": true
    }
}
